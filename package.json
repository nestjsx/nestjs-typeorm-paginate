{
  "name": "nestjs-typeorm-paginate",
  "author": {
    "name": "Ashleigh Simonelli",
    "email": "ashleighsimonelli@gmail.com",
    "url": "http://ashleighsimonelli.co.uk"
  },
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nestjsx/nestjs-typeorm-paginate"
  },
  "description": "A simple pagination function to build a pagination object with types",
  "keywords": [
    "nestjs",
    "typeorm",
    "pagination",
    "paginate"
  ],
  "devDependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/testing": "^11.0.1",
    "@nestjs/typeorm": "^11.0.0",
    "@types/jest": "^27.0.0",
    "@types/node": "^20.9.0",
    "cross-env": "^7.0.3",
    "jest": "^29.0.0",
    "mysql": "^2.17.1",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^6.0.1",
    "rxjs": "^7.1.0",
    "sqlite3": "^5.1.7",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.7.3"
  },
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "setup:test": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:sync -d ./src/__tests__/data-source.ts",
    "test": "jest --runInBand",
    "test:cov": "jest --coverage",
    "build:clean": "rimraf ./dist || rm -rf ./dist",
    "build:tsc": "tsc",
    "build:index:clean": "rimraf ./index.js ./index.d.ts || rm -rf ./index.js ./index.d.ts",
    "build:index:tsc": "tsc -d --skipLibCheck ./index.ts",
    "build": "npm-run-all2 -s build:clean build:tsc build:index",
    "build:index": "npm-run-all2 -s build:index:clean build:index:tsc",
    "prepublish": "npm-run-all2 -s format build"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.1.1 || ^5.6.2 || ^7.0.0 || ^8.0.0 || ^9.0.0 || ^10.0.0 || ^11.0.0",
    "typeorm": "^0.3.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "src/__tests__"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  },
  "version": "4.0.4",
  "overrides": {
    "tough-cookie": "^4.1.3",
    "semver": "^7.5.4",
    "word-wrap": "^1.2.5",
    "@babel/traverse": "^7.23.2",
    "request": "2.88.2"
  }
}
